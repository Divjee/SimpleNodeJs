<html>
  <head>
    <style>
      .header {
        background-color: rgb(245, 238, 238);
        border-color: black;
        border: 5px solid black;
        padding: 10px;
        display: flex;
        justify-content: left;
        align-items: center;
        height: 40px;
        margin-top: 5px;
        margin-left: 5px;
        margin-right: 5px;
      }
  
      .header a {
        color: rgb(12, 11, 11);
        text-decoration: none;
      }
  
      .header a:last-child {
        margin-right: 0px;
      }
  
      .header .divider {
        color: rgb(21, 18, 18);
        margin: 0 10px;
      }
      .container{
        border: 5px solid black

       }
       .header1 {
        background-color: rgb(245, 238, 238);
        border-color: black;
        border: 5px solid black;
        padding: 10px;
       align-items: center;
        flex-direction: column;
      display: flex;
        justify-content: center;
        height: 750px;
        margin-top: 8px;
        margin-left: 5px;
        margin-right: 5px;
        margin-bottom: 8px;
        height: max-content;
       
      }

      .item-div {
        width: 200px;
        height: 50px;
        border: 1px solid black;
        display: flex;
        justify-content: center;
        align-items: center;
        margin-bottom: 10px;
      }
    </style>
  </head>
  <body>
    <div class="container">
      <div class="header">
        <a href="/">Main menu</a>
        <span class="divider">|</span>
        <a href="/menu1">Menu 1</a>
        <span class="divider">|</span>
        <a href="/menu2">Menu 2</a>
      </div>
      <div class="header1">
        <h1>Menu 2</h1>
        <% for (const item of items) { %>
          <div class="item-div">
            <p>ID: <%= item.id%> </p>
            <p>Value: <%= item.value %></p>
          </div>
        <% } %>
      </div>
      <h3>Web application</h3>
    </div>
    </body>
</html>
